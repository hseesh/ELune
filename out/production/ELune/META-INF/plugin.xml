<idea-plugin>
  <id>com.yatoufang.hse.code.generate.scanner</id>
  <name>ELune</name>
  <version>1.04</version>
  <vendor email="gong-huang@qq.com" url="https://github.com/hseesh">gonghuang</vendor>

  <description><![CDATA[
   <p>
    <b>Automatically generate Documentation for JAVA Web API</b><br/>
    <b>Http Test Client</b>
    </p>
    ]]></description>

  <change-notes>
    <![CDATA[
        Known bug fix<br/>
        Http test support<br/>
    ]]>
  </change-notes>


  <idea-version since-build="183.0"/>

  <depends>com.intellij.modules.platform</depends>
  <depends config-file="depends.xml" optional="true">com.intellij.java</depends>
  <depends config-file="module.xml" optional="true">com.intellij.modules.java</depends>

  <actions>
        <action id="com.yatoufang.code.generator.scanner" class="com.yatoufang.action.TableScannerAction" text="Scanner">
          <add-to-group group-id="EditorPopupMenu" anchor="after" relative-to-action="$Paste"/>
        </action>
      <action id="com.yatoufang.hse.Elune.NewTable" class="com.yatoufang.action.NewTableAction" text="New Table">
          <add-to-group group-id="NewGroup" anchor="last"/>
      </action>
  </actions>


  <extensions defaultExtensionNs="com.intellij">


    <toolWindow id="ELune"
                anchor="bottom"
                canCloseContents="true"
                factoryClass="com.yatoufang.config.ConsoleWindowFactory"/>

    <postStartupActivity implementation="com.yatoufang.action.StartUpAction"/>

  </extensions>

</idea-plugin>