<templateSet group="ELUNE_PLUS">
    <template
            name="valueOf"
            value="$valueOf$"
            description="Elune: public static &lt;T&gt; valueOf(Object..)"
            toReformat="true"
            toShortenFQNames="false">
        <variable
                name="valueOf"
                expression="valueOf(SELECTION)"
                alwaysStopAt="true"/>
        <context>
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>

    <template
            name="valueT"
            value="$valueT$"
            description="Elune: public static &lt;TVO&gt; valueOf(&lt;T&gt;)"
            toReformat="true"
            toShortenFQNames="false">
        <variable
                name="valueT"
                expression="valueT(SELECTION)"
                alwaysStopAt="true"/>
        <context>
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>

    <template
            name="checkMe"
            value="$checkMe$"
            description="Elune: If null or fail then return"
            toReformat="true"
            toShortenFQNames="false">
        <variable
                name="checkMe"
                expression="checkMe(suggestFirstVariableName(&quot;Object&quot;))"
                alwaysStopAt="true"/>
        <context>
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>


    <template
            name="invalidParam"
            value="$invalidParam$"
            description="Elune: return Invalid Param"
            toReformat="true"
            toShortenFQNames="false">
        <variable
                name="invalidParam"
                expression="invalidParam()"
                alwaysStopAt="true"/>
        <context>
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>

    <template name="_list" value="List&lt;$PARAM$&gt; $NAME$ = Lists.newArrayList();"
              description="Elune: List&lt;$PARAM$&gt; list = Lists.newArrayList()" toReformat="true"
              toShortenFQNames="true">
        <variable name="NAME" expression="" defaultValue="list" alwaysStopAt="true"/>
        <variable name="PARAM" expression="" defaultValue="&quot;Object&quot;" alwaysStopAt="true"/>
        <context>`
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>

    <template name="_set" value="Set&lt;$PARAM$&gt; $NAME$ = Sets.newHashSet();"
              description="Elune: Set&lt;$PARAM$&gt; set = Sets.newHashSet()" toReformat="true" toShortenFQNames="true">
        <variable name="NAME" expression="" defaultValue="set" alwaysStopAt="true"/>
        <variable name="PARAM" expression="" defaultValue="&quot;Object&quot;" alwaysStopAt="true"/>
        <context>
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>

    <template name="_map" value="Map&lt;$PARAM$,$PARAM1$&gt; $NAME$ = Maps.newHashMap();"
              description="Elune: Map&lt;Object,Object&gt; map = Maps.newHashMap()" toReformat="true"
              toShortenFQNames="true">
        <variable name="NAME" expression="" defaultValue="map" alwaysStopAt="true"/>
        <variable name="PARAM" expression="" defaultValue="&quot;Object&quot;" alwaysStopAt="true"/>
        <variable name="PARAM1" expression="" defaultValue="&quot;Object&quot;" alwaysStopAt="true"/>
        <context>
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>
    <template
            name="sso"
            value="int ServerType, int ServerId, int data, String openDate"
            description="int ServerType, int ServerId, int data, String openDate"
            toReformat="true"
            toShortenFQNames="false">
        <context>
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>
    <template
            name="logger"
            value="LOGGER.error(&quot;$END$&quot;);"
            description="LOGGER.error();"
            toReformat="true"
            toShortenFQNames="false">
        <context>
            <option name="ELUNE_PLUS" value="true"/>
        </context>
    </template>
</templateSet>